{
  "name": "alexedwards-co",
  "private": true,
  "description": "Website setup with Cypress (End-to-End testing), Jest (Unit & Integration testing) and Typescript (Type validation)",
  "version": "0.1.0",
  "author": "Alex Edwards <alex@alexedwards.co> (https://alexedwards.co)",
  "dependencies": {
    "dotenv": "^8.2.0",
    "gatsby": "^2.21.22",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-catch-links": "^2.3.3",
    "gatsby-plugin-manifest": "^2.4.2",
    "gatsby-plugin-offline": "^3.2.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-plugin-styled-components": "^3.3.2",
    "gatsby-plugin-ts": "^2.7.1",
    "gatsby-remark-code-titles": "^1.1.0",
    "gatsby-remark-copy-linked-files": "^2.3.3",
    "gatsby-remark-images-contentful": "^2.3.3",
    "gatsby-remark-prismjs": "^3.5.2",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-source-contentful": "^2.3.12",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-remark": "^2.8.13",
    "gatsby-transformer-sharp": "^2.5.2",
    "prismjs": "^1.20.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "rehype-react": "^5.0.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.8.3",
    "@babel/types": "^7.9.6",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/rehype-react": "^4.0.0",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "acorn": "^6.4.1",
    "axe-core": "^3.5.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-gatsby": "^0.4.2",
    "cypress": "^4.5.0",
    "cypress-axe": "^0.8.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-cypress": "^2.9.0",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.1",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.0.0",
    "jest": "^26.0.1",
    "jest-axe": "^3.2.0",
    "jest-in-case": "^1.0.2",
    "jest-runner-eslint": "^0.8.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.5",
    "start-server-and-test": "^1.10.8",
    "test-data-bot": "^0.8.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.2"
  },
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "typescript",
    "cypress",
    "jest"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test serve http://localhost:9000 cy:run",
    "test:e2e:dev": "start-server-and-test develop http://localhost:8000 cy:open",
    "type-check": "tsc --pretty --noEmit",
    "lint": "jest --config test/jest.lint.js",
    "validate": "npm run test:coverage && npm run test:e2e:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lexedwards/starter-cypr-jest-ts"
  },
  "bugs": {
    "url": "https://github.com/lexedwards/starter-cypr-jest-ts/issues"
  }
}
